<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xmlns       ="https://rdf.muninn-project.org/ontologies/graves#"
    xmlns:person="http://rdf.muninn-project.org/ontologies/person#"
    xmlns:schema="https://schema.org/"
    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:org="http://rdf.muninn-project.org/ontologies/organization#"
    xmlns:mil="http://rdf.muninn-project.org/ontologies/military#"
    xmlns:muninn="http://rdf.muninn-project.org/ontologies/muninn#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:religion="http://rdf.muninn-project.org/ontologies/religion#"
    xmlns:dbpedia="http://dbpedia.org/resource/"
    xmlns:graves="https://rdf.muninn-project.org/ontologies/graves#"
    xmlns:time = "http://www.w3.org/2006/time#"   
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:loc="http://id.loc.gov/vocabulary/relators/"
    xmlns:skos="http://www.w3.org/2004/02/skos/core#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
    xmlns:documents="http://rdf.muninn-project.org/ontologies/documents#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:void="http://rdfs.org/ns/void#"
    xmlns:prov="http://www.w3.org/ns/prov#"
    xmlns:dcat="http://www.w3.org/ns/dcat#"
    xmlns:bibo="http://purl.org/ontology/bibo/"   
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:cc="http://creativecommons.org/ns#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:geosparql="http://www.opengis.net/ont/geosparql#"
    xmlns:sf="http://www.opengis.net/ont/sf#"
    xml:base="https://rdf.muninn-project.org/ontologies/graves#">
<graves:Cenotaph>
 <!-- The Cenotaph itself -->
 <owl:sameAs rdf:resource="http://dbpedia.org/resource/National_War_Memorial_(Canada)"/>
 <!-- DBPedia does use characters like '(' that can be mangled by parsers in urls -->
 <graves:hasLocation>
  <geo:Point>
   <geo:lat>45.424036</geo:lat>
   <geo:long>-75.695497</geo:long>
  </geo:Point>
 </graves:hasLocation>
 <graves:siteName xml:lang="en">National War Memorial</graves:siteName>
 <rdfs:label xml:lang="en">National War Memorial (Canada)</rdfs:label>
 <graves:siteName xml:lang="fr">Monument commémoratif de guerre du Canada</graves:siteName>
 <rdfs:label xml:lang="fr">Monument commémoratif de guerre du Canada</rdfs:label>
 <graves:isCommemorationOf rdf:resource="#EraGreatWar"/>
 <graves:isCommemorationOf rdf:resource="#KoreanWar"/>
 <graves:isCommemorationOf rdf:resource="#EraSecondWorldWar"/>
 <graves:isCommemorationOf>
  <foaf:Group>
   <foaf:name>Canadian War Veterans</foaf:name>
  </foaf:Group>
 </graves:isCommemorationOf>         
 <graves:hasPart>  
  <graves:Sarcophagus rdf:about="#theSarcophagus">
   <!-- The Sarcophagus containing the Unknown Soldier -->
   <graves:siteName>Tomb of the Unknown Soldier</graves:siteName>
   <owl:sameAs rdf:resource="http://en.wikipedia.org/wiki/Canadian_Tomb_of_the_Unknown_Soldier"/>
   <graves:containsRemains>
    <graves:Skeleton>
     <!-- Unknown Soldier's Remains -->
     <graves:hisRemains>
      <graves:Soldier>
       <rdfs:label xml:lang="en">An Unknown Canadian Soldier of the First World War</rdfs:label>
      </graves:Soldier>
     </graves:hisRemains>
     <graves:movedFrom rdf:resource="#previousRestingPlace"/>
    </graves:Skeleton>
   </graves:containsRemains>
  <graves:startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">20000528</graves:startDate>
  </graves:Sarcophagus>
 </graves:hasPart>
 <graves:startDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">19381038</graves:startDate>
 <prov:wasAssociatedWith rdf:resource="http://en.wikipedia.org/wiki/Vernon_March"/>
 <!-- Vernon March designed the Cenotaph --> 
</graves:Cenotaph>
</rdf:RDF>
